{
  "expo": {
    "name": "Scrum Tools",
    "description": "Scrum standup + poker cards tools mobile app.",
    "slug": "scrum-tools",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2D3FDC"
